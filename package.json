{
  "name": "worktimer",
  "version": "1.2.3",
  "description": "timer for keeping work-time at home",
  "main": "./public/main.js",
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "productName": "Youl",
    "appId": "com.sample.worktimer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "public/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "public/icon/icon.icns",
      "target": [
        "dmg"
      ]
    }
  },
  "keywords": [
    "timer"
  ],
  "author": "yuta-ike",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.4.1",
    "html-webpack-plugin": "^4.0.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "css-loader": "^3.5.2",
    "dayjs": "^1.8.23",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1"
  }
}
